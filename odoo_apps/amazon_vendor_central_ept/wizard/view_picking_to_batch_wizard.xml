<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_picking_to_batch_wizard" model="ir.ui.view">
        <field name="name">Select Batch</field>
        <field name="model">picking.to.batch.wizard</field>
        <field name="arch" type="xml">
            <form>
            	<group>
            		<field name="batch_id" options="{'no_create':True}"></field>
            		<field name="picking_ids"></field>
	           	</group>
	           	<footer>
                <button string="Print" name="batch_report" class="oe_highlight"  type="object">
                 </button>  
                 <button string="Cancel" class="oe_highlight"  type="object" 
                            name="action_cancel">
                  </button>
                        
	           	</footer>
            </form>   
        </field>
    </record>
</odoo>